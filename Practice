cd C:\Program Files\Microsoft Visual Studio\2022\Community\VC\Auxiliary\Build\
vcvars64.bat 
vcvars64
notepad mycp.c
(안에 소스코드 넣기)
#include <stdio.h>
int main(int argc, char *argv[]) {
    FILE *s, *t;
    char c;
    s = fopen(argv[1], "r");
    t = fopen(argv[2], "w");
    while ((c = fgetc(s)) != EOF)
        fputc(c, t);
    fclose(s);
    fclose(t);
    return 0;
}
cl mycp.c
mycp f1.txt f2.txt
